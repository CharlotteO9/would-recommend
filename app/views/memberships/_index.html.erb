<div class="container">
  <h1>Membership index</h1>
  <%= link_to "Add a new member", new_community_membership_path(@community) %>
  <ul><% @memberships.each do |membership| %>
    <li>
      <div class="avatar">
        <img src="<%= membership.user.avatar %>" alt="profil picture">
      </div>
      <p><%= membership.user.username %> </p>
    </li>
    <% end %>
  </ul>
</div>
